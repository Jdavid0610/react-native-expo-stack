{
  "expo": {
    "name": "expo-stack",
    "slug": "expo-stack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-placeholder.png",
    "userInterfaceStyle": "light",
    "scheme": "expo-stack",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-placeholder.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expostack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-placeholder.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.expostack.app"
    },
    "web": {
      "favicon": "./assets/favicon-placeholder.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-secure-store"]
  }
}
